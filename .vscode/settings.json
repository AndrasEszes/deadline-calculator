{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.rulers": [
    120
  ],
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[json]": {
    "files.insertFinalNewline": true
  }
}